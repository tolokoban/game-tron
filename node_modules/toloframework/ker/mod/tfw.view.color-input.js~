"use strict";

var CODE_BEHIND = {
  onValueChanged: onValueChanged
};


var Color = require("tfw.color");


function onValueChanged( cssColor ) {
  Color.instance.parse( cssColor );
  var luminance = Color.instance.luminance();
  var color = "#000";
  if( luminance < .5 ) {
    color = "#fff";
  }
  this.$elements.button.$.style.color = color;
}
