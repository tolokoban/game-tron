"use strict";

var CODE_BEHIND = {
  onInitValueChanged: onInitValueChanged,
  onLanguageChanged: onLanguageChanged,
  onValueChanged: onValueChanged
};


var Tfw = require("$");


function onLanguageChanged( newLang ) {
  if( newLang.length != 2 ) {
    this.language = Tfw.lang();
    return;
  }

  
}
