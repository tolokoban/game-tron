"use strict";

var List = require("tfw.binding.list");


describe('tfw.binding.list', function() {
  it('should recognize a List build from an array', function(done){
    var lst = new List([]);
    expect( List.isList( lst ) ).toBe( true );
  });
  it('should recognize a List build from another List', function(done){
    var lst2 = new List([]);
    var lst = new List(lst2);
    expect( List.isList( lst ) ).toBe( true );
  });

});
